    "@middy/validator": "^1.0.0",
    "aws-xray-sdk": "^2.3.6",
    "debug-level": "^0.3.2",
    "http-errors": "^1.8.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.62",
    "@types/aws-sdk": "^2.7.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.11.1",
    "@types/uuid": "^8.3.0",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.541.0",
    "eslint": "^7.9.0",
    "eslint-plugin-prettierx": "^0.14.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "prettier-standard": "^16.4.1",
    "serverless-bundle": "3.1.0",
    "serverless-dotenv-plugin": "^2.4.2",
