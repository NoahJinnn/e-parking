    "serverless-pseudo-parameters": "^2.5.0",
    "standard": "^14.3.4",
    "ts-jest": "^26.4.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.9"
  },
  "scripts": {
    "test": "standard --env jest && serverless-bundle test",
    "fix": "standard --fix --env jest",
